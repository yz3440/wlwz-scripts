    },
    {
      "type": "dialog",
      "speaker": "莫小贝",
      "performances": [
        {
          "type": "line",
          "line": "惧……据说明天有雨！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "众人",
      "performances": [
        {
          "type": "line",
          "line": "嗯？！"
        }
