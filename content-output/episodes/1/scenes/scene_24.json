      ]
    },
    {
      "type": "dialog",
      "speaker": "莫小贝",
      "performances": [
        {
          "type": "line",
          "line": "地球人都知道。"
        }
      ]
    },
    {
      "type": "action",
      "description": "小郭向下走，众人拥作一团"
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "不要过来。苦海无边回头是岸，岸。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "按佛家来说，放下屠刀立地成佛，佛。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "李大嘴",
      "performances": [
        {
          "type": "line",
          "line": "佛法无边普渡众生，生。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "生生亦何欢死亦何惧，惧。"
