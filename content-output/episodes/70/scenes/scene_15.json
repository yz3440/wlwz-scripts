    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "哎呦，我好怕怕哦！讲话要讲证据啊！小心我告你诽谤啊！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "小翠",
      "performances": [
        {
          "type": "line",
          "line": "我还告你拐带人口呢！"
