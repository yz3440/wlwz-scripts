    {
      "type": "dialog",
      "speaker": "小翠",
      "performances": [
        {
          "type": "line",
          "line": "赶紧进来！"
        }
      ]
    },
    {
      "type": "action",
      "description": "老白惊异地看着赛"
    },
    {
      "type": "dialog",
      "speaker": "赛貂禅",
      "performances": [
        {
          "type": "line",
          "line": "白大哥，好久不见啊！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "赛掌柜，你咋成这样了？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "赛貂禅",
      "performances": [
        {
          "type": "line",
          "line": "呃，呵……一言难尽呐！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "小翠",
      "performances": [
        {
          "type": "speaker-action",
          "description": "走向赛"
        },
        {
          "type": "line",
          "line": "你还在这儿跟人搭腔了？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "赛貂禅",
      "performances": [
        {
          "type": "line",
          "line": "哎呀哎呀……"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "别别别……"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "小翠",
      "performances": [
        {
          "type": "line",
          "line": "咋跟你说的，未经本夫人允许啊，严禁跟任何人讲话！"
