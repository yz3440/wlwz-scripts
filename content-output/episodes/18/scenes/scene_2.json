      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "回去吧。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "speaker-action",
          "description": "冲食客"
        },
        {
          "type": "line",
          "line": "嗨。"
        }
      ]
