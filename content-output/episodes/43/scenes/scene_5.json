      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "吃哪个啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白三娘",
      "performances": [
        {
          "type": "line",
          "line": "吃鸡吃鸡，吃吃吃。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "众人",
      "performances": [
        {
          "type": "line",
          "line": "吃，吃。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白三娘",
      "performances": [
        {
          "type": "line",
          "line": "哎呀，豆腐是有营养的。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "众人",
      "performances": [
        {
          "type": "line",
          "line": "豆腐有营养……"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白三娘",
      "performances": [
        {
          "type": "line",
          "line": "哎这黄瓜是新摘的！"
