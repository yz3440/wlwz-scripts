  "performances": [
    { "type": "action", "description": "老白坐在井口给小贝说书" },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "李元霸不管三七二十一，上来就是一锤。伍天锡只好拿混金铛一架，震得是两手流血，掉转马头就跑。李元霸上来就伸手那么一提"
        },
        { "type": "speaker-action", "description": "手去抓小贝的梨，小贝躲开" },
        { "type": "line", "line": "——欲知后事如何，把你那鸭梨给我" },
        { "type": "action", "description": "小贝攥紧梨" },
        {
          "type": "line",
          "line": "，上来就伸手那么一提，往天空上一抛，双手接住了双脚，喀嚓——把人劈成了两半，好一个李元霸呀！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "莫小贝",
      "performances": [
        {
          "type": "line",
          "line": "别讲了，别讲了，每次都把人家撕成两半，多没意思呀。"
        },
        { "type": "speaker-action", "description": "要吃梨" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        { "type": "speaker-action", "description": "伸手拦住" },
        {
          "type": "line",
          "line": "先别吃了，换李逵。好一个黑旋风啊，手持两把宣花板斧，冲进了人群是左劈右砍"
        },
        { "type": "speaker-action", "description": "抓梨，小贝手一缩躲开" },
        { "type": "line", "line": "——待会儿再砍。" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "莫小贝",
      "performances": [{ "type": "line", "line": "什么味儿呀？" }]
    },
    { "type": "action", "description": "小郭从厨房跑出" },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [{ "type": "line", "line": "刚烤的里脊。" }]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        { "type": "line", "line": "怎么这么大的味啊，难闻死了！" }
      ]
    },
    { "type": "action", "description": "掌柜的到后院" },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [{ "type": "line", "line": "啥糊了，啥糊了？" }]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        { "type": "line", "line": "没糊，没糊，炭烤里脊就这味儿。" }
      ]
    },
    { "type": "action", "description": "镜头给到糊了的里脊" },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [{ "type": "line", "line": "炭倒是好炭，里脊在哪儿呢？" }]
