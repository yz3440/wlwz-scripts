    {
      "type": "dialog",
      "speaker": "大嘴娘",
      "performances": [
        {
          "type": "line",
          "line": "儿啊！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "李大嘴",
      "performances": [
        {
          "type": "line",
          "line": "哎！"
