      ]
    },
    {
      "type": "dialog",
      "speaker": "大嘴娘",
      "performances": [
        {
          "type": "line",
          "line": "要真是皇上赐的粥，能这么难喝吗？啊？"
        }
      ]
    },
    {
      "type": "action",
      "description": "大嘴娘、大嘴会心的笑"
    }
  ]
}