      ]
    },
    {
      "type": "dialog",
      "speaker": "莫小贝",
      "performances": [
        {
          "type": "line",
          "line": "你过来！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "这不是驴耳朵……"
        }
      ]
    }
  ]
