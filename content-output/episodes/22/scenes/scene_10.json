  "performances": [
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "娘子！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "相公！树上的鸟儿成双对。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "绿水青山带笑颜。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "从此不再受那奴役苦。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "夫妻双双把家还。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "你探案来我习武。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "我逮人来你拍砖。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "江湖虽小却多风雨。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "夫妻合作不翻船。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭、六",
      "performances": [
        {
          "type": "line",
          "line": "你我好比乌鸦鸟，比翼双飞闹人间哪——嘿！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "去去去！走！叨嘛呢，老鸹。"
        }
      ]
    }
  ]
}