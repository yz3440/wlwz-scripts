      "performances": [
        {
          "type": "line",
          "line": "留下？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "咋的，你答应啦？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "呃~"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "耶~！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "耶~！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "你又咋的啦？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "冷不丁一笑，小肚子又抽筋了。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "赶紧揉揉！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "放着我来，葵花点穴手！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "你看你咋又把他点了呢？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "他不是抽抽嘛，哎，还抽不抽啊，你要抽，你就眨眨眼睛。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "呀，他又断电了……"
