      "performances": [
        {
          "type": "line",
          "line": "楼下？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "speaker-action",
          "description": "唇语",
          "line": "留下。"
