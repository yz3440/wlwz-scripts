    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "在我这呢？"
        },
        {
          "type": "speaker-action",
          "description": "一走神剪坏了老白手"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "哎呀！"
        },
