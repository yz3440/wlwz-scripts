  "performances": [
    {
      "type": "action",
      "description": "湘玉给老白剪指甲"
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "我指甲不长。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "你不要动不要动，这么大个人了，连个指甲都弄不干净。"
