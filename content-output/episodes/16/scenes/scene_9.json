{
  "type": "normal",
  "rawDescription": "【大堂，昼】",
  "location": "大堂",
  "timeOfDay": "昼",
  "performances": [
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        { "type": "line", "line": "你刚才说要发财了，是啥意思？" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "昨天下午啊，我带秀才的书稿去了一趟左家庄。你猜我碰上谁了？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "谁呀？" }]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [{ "type": "line", "line": "范大娘啊！" }]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "范大娘谁啊？" }]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        { "type": "line", "line": "哎呀，京城最有名的书商啊。你不知道？" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "哦，三国是她发的。" }]
    },
    {
      "type": "dialog",
      "speaker": "白眉",
      "performances": [
        {
          "type": "line",
          "line": "胡说，范大娘发的那是《三国前传》，《水浒后传》和《西游外传》。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [{ "type": "line", "line": "你看人家都知道。" }]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "哦，没看过。" }]
    },
    {
      "type": "dialog",
      "speaker": "宁财神",
      "performances": [
        { "type": "line", "line": "还有《出墙记》和《俏冤家》。" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        { "type": "line", "line": "呦呦呦，二位公子，那都是禁书啊。" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [{ "type": "line", "line": "你连那都看过呢？" }]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "谁看过呀？我能像他俩似的看乱七八糟的。我跟你说啊，范大娘这种不法书商，早就该逮起来。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        { "type": "line", "line": "把她逮了，那秀才的书怎么办？" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "你还真打算让她发啊？" }]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "那当然了，范大娘说了，秀才的书，她能包销十万呢！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "那你能赚多少？" }]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [{ "type": "line", "line": "我能赚多少，你就能赚多少。" }]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "啥意思啊？" }]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        { "type": "line", "line": "秀才说他分我两成，我就分你一成。" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "为啥呀？" }]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "秀才那个死脾气，你又不是不知道，回头人家要是来了，他那倔劲一犯……"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "我就整死他。" }]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [{ "type": "line", "line": "别死，给他留口气。" }]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "留口。" }]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [{ "type": "line", "line": "我的自由，就全仰仗你了！" }]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [{ "type": "line", "line": "嗯？" }]
    }
  ]
}
