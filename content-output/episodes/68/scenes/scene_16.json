  "context": "",
  "performances": [
    {
      "type": "action",
      "description": "大伙跟辛普森谈判"
    },
    {
      "type": "dialog",
      "speaker": "辛普森",
      "performances": [
        {
          "type": "line",
          "line": "退聘？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "你先坐啊，这是无双的意思，我们也没有办法，这些聘礼麻烦你拿回去吧。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "辛普森",
      "performances": [
        {
          "type": "line",
          "line": "Wait，无双在哪里，我要见她。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "无双不想见你，你别着急啊，你看你条件这么好，找谁不行啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "辛普森",
      "performances": [
        {
          "type": "line",
          "line": "Shut up！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "李大嘴",
      "performances": [
        {
          "type": "line",
          "line": "吓大的？我告诉你，我们全是从小吓大的，怎么着你？"
