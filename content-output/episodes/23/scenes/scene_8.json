{
  "type": "normal",
  "rawDescription": "【街口，昼】",
  "location": "街口",
  "timeOfDay": "昼",
  "context": "",
  "performances": [
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "鼓掌，拿他！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "干什么？干什么！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "老白？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "干啥呢？贩卖私盐者，现已脱逃——胡扯，我刚从衙门过来，犯人在大牢里关的好好的呐，谁贴的？是不是你们？是不是你们？啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "哎哎哎，你干吗呀！——这是为了引蛇出洞。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "引什么蛇出什么洞啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "小郭出的主意。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "什么馊注意啊！她懂啥啊？她要懂怎么没人找她当捕快啊。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "切，我那是不稀的当。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "那不就得了吗！赶紧回去啊，以后不准胡闹，再被我抓着！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "李大嘴",
      "performances": [
        {
          "type": "line",
          "line": "嘿嘿，老白，好好，干得太好了，干得好！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "以后别叫老白，叫白捕头。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "李大嘴",
      "performances": [
        {
          "type": "line",
          "line": "你啥时候成捕头了？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "speaker-action",
          "description": "嘘"
        },
        {
          "type": "line",
          "line": "——自己封的。"
        }
      ]
    }
  ]
}