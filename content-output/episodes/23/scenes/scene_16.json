{
  "type": "normal",
  "rawDescription": "【大堂，昼】",
  "location": "大堂",
  "timeOfDay": "昼",
  "performances": [
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "怎么了你呀？啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "等等等等等，等到现在，他嘛时候来呀？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "你说一点红啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "那可不呗，他总不来，总得让我提心吊胆，连睡觉都睡不踏实。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "我看你睡的挺好，打起呼来，比大嘴还响。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "小六，过来。我跟你说啊，见着一点红千万别动武。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "那是为嘛？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "你想啊，一点红，武功一流，谁见谁死。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "难道看着他逍遥法外？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "六儿，你得这么想……"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "不用想了！他要是敢来，我就跟他开练，他的剑法高，我的刀法也不低！"
        }
      ]
    },
    {
      "type": "action",
      "description": "小六耍刀"
    },
    {
      "type": "dialog",
      "speaker": "一点红",
      "performances": [
        {
          "type": "line",
          "line": "好刀法。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "行啊，挺有眼光，从哪儿来啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "一点红",
      "performances": [
        {
          "type": "line",
          "line": "平谷。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "嗯？好小子！"
        }
      ]
    },
    {
      "type": "action",
      "description": "一点红拔剑在空中挥舞几下"
    },
    {
      "type": "dialog",
      "speaker": "一点红",
      "performances": [
        {
          "type": "line",
          "line": "把桌子擦干净。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "桌子刚擦的，不信您瞧……啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "怎么了？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "桌、桌上这几只苍蝇，是你、你砍下来的？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "一点红",
      "performances": [
        {
          "type": "line",
          "line": "有空房吗？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "哦，有有！客官里边儿请。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "呵呵，你那套刀法没耍完，你接着耍呀！"
        }
      ]
    }
  ]
}