    {
      "type": "dialog",
      "speaker": "众人",
      "performances": [
        {
          "type": "line",
          "line": "我跟你讲做为一个男人……"
        }
      ]
    },
    {
      "type": "action",
      "description": "众人七嘴八舌"
    },
    {
      "type": "dialog",
      "speaker": "杨蕙兰",
      "performances": [
        {
          "type": "speaker-action",
          "description": "大声"
        },
        {
          "type": "line",
          "line": "可我已经嫁人啦！"
