    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [{ "type": "line", "line": "讨～厌……" }]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [{ "type": "line", "line": "说话要有真凭实据的。" }]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [{ "type": "line", "line": "讨厌讨厌！" }]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [{ "type": "line", "line": "你到底为什么要亲我嘛？" }]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [{ "type": "line", "line": "讨厌讨厌讨厌！" }]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [{ "type": "line", "line": "啧，你能不能换个词儿啊？" }]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [{ "type": "line", "line": "讨厌讨厌讨厌讨厌讨厌！" }]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [{ "type": "line", "line": "真烦人，不跟你说了！" }]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "哎哎……好吧好吧，我告诉你，我亲你是因为我——是因为你聪明！"
