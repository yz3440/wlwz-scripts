    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [{ "type": "line", "line": "哪天啊？" }]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [{ "type": "line", "line": "就是姬无病落网的那天。" }]
