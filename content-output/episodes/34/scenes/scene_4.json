      "performances": [
        {
          "type": "speaker-action",
          "description": "剑一倒手"
        },
        {
          "type": "line",
          "line": "啪……！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "一路顺风，常回家看看，常回家看看……"
