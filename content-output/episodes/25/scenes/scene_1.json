      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "知道啦，侯哥！"
        }
      ]
    },
    {
      "type": "action",
      "description": "小贝上"
    },
    {
      "type": "dialog",
      "speaker": "莫小贝",
      "performances": [
        {
          "type": "line",
          "line": "我也要听我也要听啊！"
