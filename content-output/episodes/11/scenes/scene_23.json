    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        { "type": "speaker-action", "description": "附和" },
        { "type": "line", "line": "一样。" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "交税是咱们应尽的责任和义务。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        { "type": "speaker-action", "description": "附和" },
        { "type": "line", "line": "义务。" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "如果没有税收，国家哪有那么多的钱来维护你的各种权利？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        { "type": "speaker-action", "description": "附和" },
        { "type": "line", "line": "权利。" }
