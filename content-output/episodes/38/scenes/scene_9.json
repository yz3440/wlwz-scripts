{
  "type": "normal",
  "rawDescription": "【女寝，昼】",
  "location": "女寝",
  "timeOfDay": "昼",
  "performances": [
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "哎哟，干什么嘛？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "你要敢说出来我就一掌——我……"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "嗯？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "拍死我自己。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "你拍死你自己吧！这可是人家无双给秀才的，你凭啥扣下来？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "切，就她手艺好，就她会做衣服？就她知道疼人？走都走了还跟我来这手，缺不缺德！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "也不知道谁缺德。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "我缺德行了吧？我那点德都让耗子给啃了行了吧？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "不行，你说啥也不行。赶紧把东西还给人家。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "切！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "你不还是吧？——秀才！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "行行行我自己还！拿过来！"
        }
      ]
    }
  ]
}