    },
    {
      "type": "dialog",
      "speaker": "包大人",
      "performances": [
        {
          "type": "line",
          "line": "好好好等一下等一哈！"
        },
        {
          "type": "speaker-action",
          "description": "拿银子"
        },
        {
          "type": "line",
          "line": "够了吧？想要小费直说吧！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "我没想要啊！"
        }
