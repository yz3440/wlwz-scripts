{
  "type": "normal",
  "rawDescription": "【佟寝，昼】",
  "location": "佟寝",
  "timeOfDay": "昼",
  "performances": [
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "你快着点儿。后面还好几条呢。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "你回过头去嘛。哎呀，这一条总算过去了吧。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "开嘛玩笑，那么大的箱子，哪能随时掏啊。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "那你说咋办嘛。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "别着急啊，又没人逼你，是你自己非要抢着当试点的。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "好好好，你说咋办，都听你的。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "我师父说了，小东西，全带在身上。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "你看看，扳指，手镯，还有耳环，簪子，全都带了。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "大件儿也得带呀，比如说名人字画，古董花瓶嘛的，都带上。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "古董花瓶你随身带给我看看。来来来你带一个呀。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "别着急，话还没说完呐。我师父说了，大东西全都当了。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "当了？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "燕小六",
      "performances": [
        {
          "type": "line",
          "line": "换成当票，不就随身带在身上了吗？我没说明白吗？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "我想杀你。"
        }
      ]
    }
  ]
}