  "performances": [
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "饿了吧，来，吃点东西。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "你别过来，否则……"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "跳下去啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "我真的会跳的喔。"
