    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        { "type": "line", "line": "大嘴！" },
        { "type": "speaker-action", "description": "对小郭" },
        { "type": "line", "line": "还有啥好说的？牌子呢？" },
        { "type": "line", "line": "这是个啥嘛？" }
