    },
    {
      "type": "dialog",
      "speaker": "李大嘴",
      "performances": [{ "type": "line", "line": "我姑夫给你题的字。" }]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [{ "type": "line", "line": "就这呀？" }]
