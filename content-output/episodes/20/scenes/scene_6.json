      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "把饺子给他！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "侯三",
      "performances": [
        {
          "type": "line",
          "line": "把那盘也给我拿来。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "给他给他。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "侯三",
      "performances": [
        {
          "type": "line",
          "line": "好吃不如饺子，舒服它不如倒着！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "众人",
      "performances": [
