{
  "type": "normal",
  "rawDescription": "【客栈外，昼】",
  "location": "客栈外",
  "timeOfDay": "昼",
  "context": "",
  "performances": [
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "哎，哎，哎。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "你这就算完咧？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "那还想怎么样？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "人家那边拉客人，都是实打实的喊出来的。咱也要喊，人就乌泱乌泱都朝咱这边来了。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "好好好，同福客栈新菜上市，欢迎品尝。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "都有什么新菜啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "哎呀，刑捕头来了里边请。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "怡红楼公关",
      "performances": [
        {
          "type": "line",
          "line": "怡红楼百年老酒出窖了，欢迎来免费品尝。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "我还是呆会儿再来吧。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "哎哎哎，老邢……势利的东西，额就不相信——每位贵客送开胃小菜两碟，烧刀子一壶！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "怡红楼公关",
      "performances": [
        {
          "type": "line",
          "line": "每位贵客送油爆大虾八只！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "我送炭烤母猪蹄一个，普洱二两！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "怡红楼公关",
      "performances": [
        {
          "type": "line",
          "line": "我送长百山熊掌两只！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "每位顾客送——每人一个大红包！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "怡红楼公关",
      "performances": [
        {
          "type": "line",
          "line": "我送每人二十两纹银！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "我送杂役一个，先到先得！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "谁敢动我！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "怡红楼公关",
      "performances": [
        {
          "type": "line",
          "line": "我送丫环两个，不限量供应！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "我把自己送出去！咋不喊了，喊呀。没有辙了吧，人呢，人呢？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "我估计都到里边去领丫环了……"
        }
      ]
    }
  ]
}
