  "performances": [
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "客官请留步，首先请接受我诚挚的祝福。"
        },
        {
          "type": "reaction",
          "reactor": "邢育森",
          "description": "老刑从小道跑来"
        },
        {
          "type": "line",
          "line": "刑捕头请留步，首先请接受我……"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "别喊了，是人都到怡红楼去，谁还到你这儿来吃糠咽菜啊。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "你别再说风凉话了，再折腾两天这就关门了。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "不至于吧？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "你看这脸色多少顿没吃了，再过两天就饿死在街头了。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "不至于吧，想拉客是吧？到官道上拉去啊。每天路过那的至少三四百人。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "你净扯，那官道离这儿多远呐，谁能为吃一顿饭跑这儿来。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "你把官道一堵，咔，他不绕也得绕啊。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "一堵，咔？那不成劫道的了吗？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "年轻人，没冒险精神，怎么发大财啊？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "怡红楼公关",
      "performances": [
        {
          "type": "line",
          "line": "七百四十六号——"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "来啦来啦。万一你折进去，千万别说是我出的主意啊。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "怡红楼公关",
      "performances": [
        {
          "type": "line",
          "line": "七百四十六号——"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "来啦！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "堵官道，管道怎么堵啊。客官请留步，首先请接受我诚挚的……"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "同福客栈，品菜大会正式开始！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "——这回有吃的了！"
        }
      ]
    }
  ]
}