{
  "type": "normal",
  "rawDescription": "【大堂，昼】",
  "location": "大堂",
  "timeOfDay": "昼",
  "performances": [
    {
      "type": "dialog",
      "speaker": "怡红楼公关",
      "performances": [
        {
          "type": "line",
          "line": "恭祝怡红楼重新开张，前十名贵客饭资全免！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "开张就开张，当街放炮，就不害怕炸着人。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "人家有钱呗——有钱也不能那么糟蹋。我现在就出去教训教训他们。"
        }
      ]
    },
    {
      "type": "action",
      "description": "邢捕头入"
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "你要教训谁啊，教训谁啊？对门儿刚开张，不许胡闹啊！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "你干啥？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "我来借双筷子，对面客人太多，筷子不够使。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "筷子不外借，你就拿手抓着吃吧。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "用手抓啊？小气儿劲，怪不得生意这么差。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "怡红楼公关",
      "performances": [
        {
          "type": "line",
          "line": "来啊来啊，每位贵客送汾酒一壶，膏蟹八两，请进，请进。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "我还是用手抓吧。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "慢着，我也有东西要送。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "你又没重新开张，你这不瞎胡闹吗？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "恭祝同福客栈开张九百二十八天。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "怡红楼公关",
      "performances": [
        {
          "type": "line",
          "line": "恭祝怡红楼重新开张。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "恭祝皇上登基，七年零八个月吾皇万岁万万岁！——咋不喊了，咋进去了？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "哎呀，人家等着你喊赠品。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "赠品是吧。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "恩。"
        }
      ]
    },
    {
      "type": "action",
      "description": "佟耳语"
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "啊，这合适吗？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "有啥不合适的？快喊。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "每位贵客送咸鸭蛋两枚，花茶一杯。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "邢育森",
      "performances": [
        {
          "type": "line",
          "line": "这也忒少了，真抠门。"
        },
        {
          "type": "speaker-action",
          "description": "跑向怡红楼"
        },
        {
          "type": "line",
          "line": "——哎，膏蟹给我留两只啊。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        {
          "type": "line",
          "line": "开会。"
        }
      ]
    }
  ]
}