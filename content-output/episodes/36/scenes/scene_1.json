{
  "type": "normal",
  "rawDescription": "【大堂，昼】",
  "location": "大堂",
  "timeOfDay": "昼",
  "performances": [
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "吕秀才，你干的好事儿！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "芙妹……你听我说啊！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "没什么好说的，痛痛快快受死吧！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "放着我来……嗯……放过他，冲我来！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "好一个眉清目秀、唇红齿白的妙人啊。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "一般一般，港姐第三。你的打扮招人喜欢。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "很丑很丑，亚姐第九，全凭一双灵巧的手。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "这都什么跟什么呀？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "你的美，由内而外，活血养颜，冰清玉洁惹人怜。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "你的美，由骨到皮，感天动地，此情可待成追忆。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "还是你美。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "你才美。你全家都美。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "你气质好呀！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "你身材好呀！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "你皮肤好！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "你发质好！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "你什么都好，唯一的缺点就是太引人注目，容易引起交通事故！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "你的缺点就是太完美，完美得让人后悔！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "后悔什么呀？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "投错胎呀，没摊上好的 DNA。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "瞎客气啥呀！都是自己人，我觉得你们俩都挺美。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉、祝无双",
      "performances": [
        {
          "type": "line",
          "line": "消失"
        },
        {
          "type": "speaker-action",
          "description": "秀才消失"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "你以为这就完事啦？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "你还想干啥呀？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "要她还是要我，你自己选一个。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "选中的就留下，没选中的。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "就去死好了！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "死就死，谁怕谁呀！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "排山倒海！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "放着我来！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "够啦！子曾经曰过，三人行必有我妻。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "是必有我师吧？"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "本来挺好一事儿，一妻一妾争什么争啊！别那么自私嘛！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "爱情就是自私的，快选！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "放着我来！"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "还是选芙妹吧，总有个先来后到。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "祝无双",
      "performances": [
        {
          "type": "line",
          "line": "无双命薄，先走一步！"
        },
        {
          "type": "speaker-action",
          "description": "准备上吊"
        },
        {
          "type": "line",
          "line": "愿你们两个白头偕老，断子绝孙。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "我选你还不行吗…………芙妹……"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "好，我成全你们，"
        },
        {
          "type": "speaker-action",
          "description": "拿出尖刀准备自裁"
        },
        {
          "type": "line",
          "line": "祝你们早生贵子，白发人送黑发人。"
        }
      ]
    },
    {
      "type": "dialog",
      "speaker": "吕秀才",
      "performances": [
        {
          "type": "line",
          "line": "芙妹，行了，我死，我死还不行吗"
        },
        {
          "type": "speaker-action",
          "description": "撞到门上"
        }
      ]
    },
    {
      "type": "action",
      "description": "秀才醒满头大汗惊醒"
    }
  ]
}