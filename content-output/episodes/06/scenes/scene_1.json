  "performances": [
    { "type": "action", "description": "老白趴在桌上睡觉，小郭在擦桌子" },
    {
      "type": "dialog",
      "speaker": "佟湘玉",
      "performances": [
        { "type": "line", "line": "哪来的这么多苍蝇，小郭你是干啥吃的。" }
      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        { "type": "line", "line": "小样儿，我还治不了你了" },
