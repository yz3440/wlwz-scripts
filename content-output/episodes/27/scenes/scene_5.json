      ]
    },
    {
      "type": "dialog",
      "speaker": "郭芙蓉",
      "performances": [
        {
          "type": "line",
          "line": "等等，"
        },
        {
          "type": "speaker-action",
          "description": "亲小六"
        },
        {
          "type": "line",
          "line": "——走吧。"
