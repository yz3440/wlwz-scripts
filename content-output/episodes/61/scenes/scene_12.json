  "performances": [
    {
      "type": "action",
      "description": "恭胳膊僵直，腿还能走动"
    },
    {
      "type": "dialog",
      "speaker": "小米",
      "performances": [
        {
          "type": "line",
          "line": "恭长老？你的胳膊？白展堂！我跟你……"
