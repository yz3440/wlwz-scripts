    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "干嘛呢！！葵花！点穴手手手手啊啊啊啊啊！"
        },
        {
          "type": "speaker-action",
          "description": "连点几十下，郭芙蓉、李大嘴拦被点"
        },
        {
          "type": "line",
          "line": "闪开！"
        },
        {
          "type": "speaker-action",
          "description": "对包"
        },
        {
          "type": "line",
          "line": "我点你死穴！！！"
        }
      ]
    },
    {
      "type": "action",
      "description": "展侍卫拉住老白"
    },
    {
      "type": "dialog",
      "speaker": "展 堂",
      "performances": [
        {
          "type": "line",
          "line": "把穴给解开啦！他可是唯一的证人，你要留活口的！"
