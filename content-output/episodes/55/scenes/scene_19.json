      ]
    },
    {
      "type": "dialog",
      "speaker": "白展堂",
      "performances": [
        {
          "type": "line",
          "line": "好，我留他一命！"
        },
        {
          "type": "speaker-action",
          "description": "对老包"
        },
        {
          "type": "line",
          "line": "我问你，知道我刚才对你做了什么吗？"
        }
      ]
